{
  "dashboard": {
    "title": "DSPy Service Overview",
    "uid": "dspy-overview",
    "tags": ["dspy", "predictions", "metrics"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(dspy_predictions_total[5m])",
            "legendFormat": "{{prediction_type}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(dspy_errors_total[5m]) / rate(dspy_predictions_total[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error %"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "P95 Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dspy_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "{{prediction_type}} P95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Active Predictions",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "dspy_active_predictions",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Active Count"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(dspy_cache_hits_total[5m]) / (rate(dspy_cache_hits_total[5m]) + rate(dspy_cache_misses_total[5m])) * 100",
            "legendFormat": "{{cache_type}} Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate %"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Cache Size",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "dspy_cache_size_bytes / 1048576",
            "legendFormat": "{{cache_type}} Size (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Size (MB)"},
          {"format": "short"}
        ]
      }
    ]
  }
}
